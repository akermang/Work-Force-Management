{
  "presets": [
    ["es2015", {"modules": false}],
    "react"
  ],    
  "plugins": [
    ["transform-class-properties"],
    ["transform-runtime"],
    ["transform-object-rest-spread"],
    ["transform-object-assign"],
    ["transform-async-to-generator"],
    ["syntax-async-functions"],
    "react-hot-loader/babel"
  ]
}